{
  "name": "version-sync",
  "version": "3.1.0",
  "description": "Automatically sync version and generate changelogs with Git tags.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsup src/**",
    "prepublishOnly": "npm run build",
    "version-sync": "node dist/index.js",
    "postinstall": "chmod +x dist/index.js"
  },
  "bin": {
    "version-sync": "./dist/index.js"
  },
  "keywords": [
    "versioning",
    "changelog",
    "git",
    "tags"
  ],
  "author": "sudo-victor",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.11.1",
    "@types/yargs": "^17.0.32",
    "dotenv": "^16.0.1",
    "inquirer": "^8.2.4",
    "openai": "^4.45.0",
    "tsup": "^8.0.2",
    "tsx": "^4.10.2",
    "typescript": "^5.4.5",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.12.11"
  }
}